<p id="notice"><%= notice %></p>

<h1>Family Relations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @family_relations.each do |family_relation| %>
      <tr>
        <td><%= family_relation.name %></td>
        <td><%= link_to 'Show', family_relation %></td>
        <td><%= link_to 'Edit', edit_family_relation_path(family_relation) %></td>
        <td><%= link_to 'Destroy', family_relation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Family Relation', new_family_relation_path %>
