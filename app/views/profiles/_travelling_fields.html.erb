  <fieldset>
    <div class="nested-fields">
      <div class="form-inline">
        <div class="form-group">
          <%= f.label :country, "Nama"%>
          <%= f.text_field :country, class: "form-control", placeholder: "Negara" %>
        </div>
        <div class="form-group">
          <%= f.label :start_date, "Dari" %>
          <%= f.date_select :start_date, { order: [:day, :month, :year], start_year: Date.today.year, end_year: Date.today.year - 70 }, { class: "form-control" }%>
        </div>
        <div class="form-group">
          <%= f.label :end_date, "Sampai" %>
          <%= f.date_select :end_date, { order: [:day, :month, :year], start_year: Date.today.year, end_year: Date.today.year - 70 }, { class: "form-control" } %>

        </div>
        <div class="form-group">
          <%= f.label :objective, "Tujuan Kunjungan" %>
          <%= f.text_area :objective, class: "form-control", placeholder: "Tujuan Kunjungan" %>
        </div>




        <div class="form-group">
          <%= f.label :name, "Hapus"%>
          <%= link_to_remove_association '', f , class: 'btn btn-sm btn-danger  glyphicon glyphicon-trash' %>  
        </div>
      </div>
    </div>
  </fieldset>


        
