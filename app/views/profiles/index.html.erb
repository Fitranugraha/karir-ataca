<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>User</th>
      <th>Id no</th>
      <th>Nationality</th>
      <th>Tribe</th>
      <th>Birth place</th>
      <th>Birth date</th>
      <th>Age</th>
      <th>Height</th>
      <th>Weight</th>
      <th>Blood</th>
      <th>Sex</th>
      <th>Married</th>
      <th>Religion</th>
      <th>Id address</th>
      <th>Current address</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Mobile phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.id %></td>
        <td><%= profile.name %></td>
        <td><%= profile.user_id %></td>
        <td><%= profile.id_no %></td>
        <td><%= profile.nationality %></td>
        <td><%= profile.tribe %></td>
        <td><%= profile.birth_place %></td>
        <td><%= profile.birth_date %></td>
        <td><%= profile.age %></td>
        <td><%= profile.height %></td>
        <td><%= profile.weight %></td>
        <td><%= profile.blood %></td>
        <td><%= profile.sex %></td>
        <td><%= profile.married %></td>
        <td><%= profile.religion %></td>
        <td><%= profile.id_address %></td>
        <td><%= profile.current_address %></td>
        <td><%= profile.email %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.mobile_phone %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
