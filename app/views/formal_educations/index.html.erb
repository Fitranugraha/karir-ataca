<p id="notice"><%= notice %></p>

<h1>Formal Educations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Level</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Certificate</th>
      <th>Faculty</th>
      <th>Department</th>
      <th>Notes</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @formal_educations.each do |formal_education| %>
      <tr>
        <td><%= formal_education.name %></td>
        <td><%= formal_education.level %></td>
        <td><%= formal_education.start_date %></td>
        <td><%= formal_education.end_date %></td>
        <td><%= formal_education.certificate %></td>
        <td><%= formal_education.faculty %></td>
        <td><%= formal_education.department %></td>
        <td><%= formal_education.notes %></td>
        <td><%= formal_education.profile %></td>
        <td><%= link_to 'Show', formal_education %></td>
        <td><%= link_to 'Edit', edit_formal_education_path(formal_education) %></td>
        <td><%= link_to 'Destroy', formal_education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Formal Education', new_formal_education_path %>
