<p id="notice"><%= notice %></p>

<h1>Scientific Works</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Notes</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scientific_works.each do |scientific_work| %>
      <tr>
        <td><%= scientific_work.title %></td>
        <td><%= scientific_work.notes %></td>
        <td><%= scientific_work.profile %></td>
        <td><%= link_to 'Show', scientific_work %></td>
        <td><%= link_to 'Edit', edit_scientific_work_path(scientific_work) %></td>
        <td><%= link_to 'Destroy', scientific_work, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scientific Work', new_scientific_work_path %>
