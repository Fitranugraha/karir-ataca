<p id="notice"><%= notice %></p>

<h1>Job Experiences</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>Company address</th>
      <th>Company phone</th>
      <th>Job title</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Job description</th>
      <th>Reason of resignation</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_experiences.each do |job_experience| %>
      <tr>
        <td><%= job_experience.company_name %></td>
        <td><%= job_experience.company_address %></td>
        <td><%= job_experience.company_phone %></td>
        <td><%= job_experience.job_title %></td>
        <td><%= job_experience.start_date %></td>
        <td><%= job_experience.end_date %></td>
        <td><%= job_experience.job_description %></td>
        <td><%= job_experience.reason_of_resignation %></td>
        <td><%= job_experience.profile %></td>
        <td><%= link_to 'Show', job_experience %></td>
        <td><%= link_to 'Edit', edit_job_experience_path(job_experience) %></td>
        <td><%= link_to 'Destroy', job_experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Experience', new_job_experience_path %>
