<p id="notice"><%= notice %></p>

<h1>Travellings</h1>

<table>
  <thead>
    <tr>
      <th>Country</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Objective</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @travellings.each do |travelling| %>
      <tr>
        <td><%= travelling.country %></td>
        <td><%= travelling.start_date %></td>
        <td><%= travelling.end_date %></td>
        <td><%= travelling.objective %></td>
        <td><%= travelling.profile %></td>
        <td><%= link_to 'Show', travelling %></td>
        <td><%= link_to 'Edit', edit_travelling_path(travelling) %></td>
        <td><%= link_to 'Destroy', travelling, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Travelling', new_travelling_path %>
